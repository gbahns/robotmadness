{
  "name": "robotmadness-online",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch server.ts",
    "dev:clean": "scripts\\dev.bat",
    "build": "next build && npm run db:generate",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "next lint --max-warnings 100",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:generate:sqlite": "prisma generate --schema=./prisma/schema.sqlite.prisma",
    "db:deploy": "prisma migrate deploy",
    "db:migrate": "prisma migrate dev",
    "db:migrate:sqlite": "prisma migrate dev --schema=./prisma/schema.sqlite.prisma",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:push:sqlite": "prisma db push --schema=./prisma/schema.sqlite.prisma",
    "db:reset": "prisma migrate reset",
    "postinstall": "npm run db:generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.14.0",
    "express": "^4.21.2",
    "mongodb": "^6.18.0",
    "next": "15.4.5",
    "next-auth": "^5.0.0-beta.29",
    "react": "19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typescript": "5.8.3",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "jest": "^30.0.5",
    "prisma": "^6.14.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5"
  }
}
